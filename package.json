{
    "name": "my-new-project",
    "type": "module",
    "devDependencies": {
        "@wdio/allure-reporter": "^8.21.0",
        "@wdio/cli": "^8.22.1",
        "@wdio/cucumber-framework": "^8.22.0",
        "@wdio/local-runner": "^8.22.1",
        "@wdio/spec-reporter": "^8.21.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "wdio": "wdio run ./wdio.conf.ts",
        "report:generate": "npx allure generate --clean ./out/allure-result/ -o ./out/allure-report && npm run saveHistory",
        "report:open": "npx allure open out/allure-report",
        "saveNewHistory": "copyfiles -f out/allure-report/history/*.json c:\\allureHistory",
        "restoreHistory": "copyfiles -f c:\\allureHistory\\*.json out/allure-result/history",
        "report": "npm run restoreHistory && npm run report:generate",
        "rerun": "call ./reRun/rerun.bat"
    },
    "dependencies": {
        "@types/node": "^20.9.0",
        "allure-commandline": "^2.24.1",
        "allure-cucumberjs": "^2.9.2",
        "chai": "^4.3.10",
        "copyfiles": "^2.4.1",
        "minimist": "^1.2.8",
        "wdio-rerun-service": "^2.0.7"
    }
}
